#include<iostream.h>
#include<string.h>
#include<conio.h>
class Vehicle
{
  public: 
    string brand = "Ford";
    void honk()
    {
      cout << "Tuut, tuut! \n" ;
    }
};


class Car: public Vehicle 
{
  public: 
    string model = "Mustang";
};

int main()
{
  Car myCar;
  myCar.honk();
  cout << myCar.brand + " " + myCar.model;
  return 0;
}
